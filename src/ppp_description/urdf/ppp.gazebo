      </plugin>
   </sensor>  
</gazebo>

<gazebo reference="sonyc__1__1">
  <material>${body_color}</material>
  <mu1>0.2</mu1>
  <mu2>0.2</mu2>
  <selfCollide>true</selfCollide>
  <selfCollide>true</selfCollide>
    <sensor type="ray" name="TeraRanger1">
